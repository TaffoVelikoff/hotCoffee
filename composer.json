{
    "name": "taffovelikoff/hotcoffee",
    "description": "A simple laravel admin panel to kick start your production as a freelancer.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taffo Velikoff",
            "email": "contact@taffovelikoff.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
		"bnbwebexpertise/laravel-attachments": "^1.0",
		"league/glide-laravel": "^1.0",
		"intervention/image": "^2.5",
		"maatwebsite/excel": "^3.1",
		"spatie/laravel-db-snapshots": "^1.2",
		"spatie/laravel-translatable": "^4.1",
		"spatie/valuestore": "^1.2",
		"rtconner/laravel-tagging": "~3.2",
		"unisharp/laravel-filemanager": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "TaffoVelikoff\\HotCoffee\\": "src/"
        },
        "files": [
            "src/Http/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TaffoVelikoff\\HotCoffee\\Tests\\": "tests/"
        }
    },
	"extra": {
        "laravel": {
            "providers": [
                "TaffoVelikoff\\HotCoffee\\HotCoffeeServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^4.0",
        "orchestra/testbench-browser-kit": "^4.0",
        "phpunit/phpunit": "^8"
    }
}
