{
    "name": "taffovelikoff/hotcoffee",
    "description": "A simple laravel admin panel to kick start your new web app.",
    "homepage": "https://github.com/TaffoVelikoff/hotcoffee",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Taffo Velikoff",
            "email": "contact@taffovelikoff.com",
            "homepage": "https://taffovelikoff.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
    "php": "^8.3",
		"illuminate/support": "^11.0|^12.0",
		"illuminate/database": "^11.0|^12.0",
		"spatie/laravel-translatable": "^6.12",
		"maatwebsite/excel": "^3.1.60",
		"intervention/image": "^3.0",
		"intervention/image-laravel": "^1.0",
		"spatie/valuestore": "^1.3",
		"league/glide-laravel": "^1.0",
		"laravel/helpers": "^1.7",
		"bnbwebexpertise/laravel-attachments": "^1.6",
		"taffovelikoff/laravel-sef": "^1.0"
	},
    "require-dev": {
        "laravel/framework": "^12.0",
        "phpunit/phpunit": "^11.0",
        "nunomaduro/collision": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "TaffoVelikoff\\HotCoffee\\": "src/",
            "TaffoVelikoff\\HotCoffee\\Database\\Seeds\\": "database/seeds/"
        },
        "files": [
            "src/Http/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TaffoVelikoff\\HotCoffee\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TaffoVelikoff\\HotCoffee\\HotCoffeeServiceProvider",
                "TaffoVelikoff\\HotCoffee\\HotCoffeeViewServiceProvider"
            ]
        }
    }
}